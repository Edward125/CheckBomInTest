!   if Message$ <> "" then  print tab(5);Message$
!   call Update_Status (Status_Code, All_Failed)
subend

sub Disconnect_FXT_Power_Supplies
subend

sub Panel_Initialize_BoardSet(State)
global BoardSet(*)
global Number_Boards_On_Panel

   for Board_Num = 1 to Number_Boards_On_Panel
      BoardSet(Board_Num) = State
   next Board_Num
subend

sub Panel_Enable_Boards
global BoardSet(*)
global BoardSet_flash(*)
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_2_to_2(*)

!TPW: This subroutine is used by debug and is also provided for
!TPW: use in custom testplans.
   for Board_Num = 1 to 1
      BoardSet_boards_1_to_1(Board_Num) = BoardSet(Board_Num)
      BoardSet_flash(Board_Num) = BoardSet(Board_Num)
   next Board_Num
   for Board_Num = 2 to 2
      BoardSet_boards_2_to_2(Board_Num) = BoardSet(Board_Num)
      BoardSet_flash(Board_Num) = BoardSet(Board_Num)
   next Board_Num
subend

sub Panel_Disable_Boards
global Number_Boards_On_Panel

!TPW: This subroutine is supplied for use in custom testplans.
!TPW: Note that BoardSet(*) is not modified - use this only in
!TPW: test subroutines. BoardSet(*) will be modified in the
!TPW: testmain after returning from this subroutine.
   for Board_Num = 1 to Number_Boards_On_Panel
      board number is Board_Num
      if boardfailed then call Panel_Disable_Single_Board(Board_Num)
   next Board_Num
subend

sub Panel_Disable_Single_Board(Board_Num)
global BoardSet_flash(*)
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_2_to_2(*)

!TPW: This subroutine is supplied for use in custom testplans.
   if (Board_Num >= 1) and (Board_Num <= 1) then
      BoardSet_boards_1_to_1(Board_Num) = 0
   end if
   if (Board_Num >= 2) and (Board_Num <= 2) then
      BoardSet_boards_2_to_2(Board_Num) = 0
   end if
subend

sub Flash_Enable_Boards
global BoardSet_flash(*)
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_2_to_2(*)

!TPW: This subroutine is used by flash and is also provided for
!TPW: use in custom testplans.
  for Board_Num = 1 to 1
    BoardSet_boards_1_to_1(Board_Num) = BoardSet_flash(Board_Num)
  next Board_Num
  for Board_Num = 2 to 2
    BoardSet_boards_2_to_2(Board_Num) = BoardSet_flash(Board_Num)
  next Board_Num
subend

sub Flash_Disable_Single_Board(Board_Num)
global BoardSet_flash(*)
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_2_to_2(*)

!TPW: This subroutine is supplied for use in custom testplans.
  if (Board_Num >= 1) and (Board_Num <= 1) then
    BoardSet_boards_1_to_1(Board_Num) = 0
    BoardSet_flash(Board_Num) = 0
  end if
  if (Board_Num >= 2) and (Board_Num <= 2) then
    BoardSet_boards_2_to_2(Board_Num) = 0
    BoardSet_flash(Board_Num) = 0
  end if
subend

sub Characterize
global All_Failed, Failed_In_Learn
global BoardSet_boards_1_to_1(*)
global BoardSet_boards_2_to_2(*)

   call Panel_Initialize_BoardSet(1)
   call Panel_Enable_Boards
   learn capacitance on
!   test "analog/c1217" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/c1701" on boards BoardSet_boards_1_to_1(*)
!   test "analog/c2402" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
!   test "analog/c2403" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
!   test "analog/c2418" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
!   test "analog/c2421" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/c2422" on boards BoardSet_boards_1_to_1(*)
   test "analog/c2603" on boards BoardSet_boards_1_to_1(*)
   test "analog/c2606" on boards BoardSet_boards_1_to_1(*)
!   test "analog/c2608" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
!   test "analog/c2609" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
!   test "analog/c2610" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/c2723" on boards BoardSet_boards_1_to_1(*)
!   test "analog/c2901" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/q2901%cgs" on boards BoardSet_boards_1_to_1(*)
   test "analog/c3401" on boards BoardSet_boards_1_to_1(*)
   test "analog/c3402" on boards BoardSet_boards_1_to_1(*)
!   test "analog/c5208" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/c5807" on boards BoardSet_boards_1_to_1(*)
   test "analog/c6203" on boards BoardSet_boards_1_to_1(*)
!   test "analog/c6401" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/q8302%cgs" on boards BoardSet_boards_1_to_1(*)
   test "analog/pq8304%c12" on boards BoardSet_boards_1_to_1(*)
!   test "analog/c9602" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
!   test "analog/ec701" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/ec702" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec1502" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec1701" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec1702" on boards BoardSet_boards_1_to_1(*)
!   test "analog/ec1704" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/ec1705" on boards BoardSet_boards_1_to_1(*)
!   test "analog/ec1707" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/ec1801" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec1802" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec1901" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2501" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2502" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2503" on boards BoardSet_boards_1_to_1(*)
!   test "analog/ec2602" on boards BoardSet_boards_1_to_1(*)  ! test commented in testorder
   test "analog/ec2701" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2708" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2901" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2902" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2903" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2904" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2905" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2906" on boards BoardSet_boards_1_to_1(*)
   test "analog/ec2907" on boards BoardSet_boards_1_to_1(*)
